#this problem is from gfg, - 'Dictionary and counter in Python to find winner of election'
votes = ["john", "johnny", "jackie",
           "johnny", "john", "jackie",
           "jamie", "jamie", "john",
           "johnny", "jamie", "johnny",
           "john"]

def countingitems(votes):
    dicts = {key: votes.count(key) for key in votes}

    print(dicts)

    di = dicts['john']
    for key,val in dicts.items():
        if val >= di and len(key) <= len('john'):
            print(key, ' has the max votes.')

countingitems(votes)
